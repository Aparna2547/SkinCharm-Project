<body>
        <section class="d-flex justify-content-center align-items-center main-div">
            <div class="p-3 rounded-1 frm-div  mt-5" >
                <form action="/admin/home" class="d-flex flex-column frm-form" method="post" id="loginValidate">
                   
                    <h6 class="text-center text-success">
                        <%=locals.context?context:""; %>
                    </h6>
                    <div class="d-flex align-items-center justify-content-center">
                        <div style="width:6rem">
                            <img src="/admin/assets/logo.png" alt="" class="img-fluid rounded-circle">
                        </div>
                        
                        <h2 class="pt-2 fw-bold text-center">ADMIN<br>LOGIN</h2>

                    </div>
                    <input type="text" placeholder="email" id="email"  name="email" class="border rounded border-dark py-2 mt-3">
                    <input type="password" placeholder="password" id="password" name="password" class="border rounded border-dark py-2 mt-3">
                    <div id="errorMessage" class="d-flex justify-content-center text-danger mt-2"></div>
                    <div class="d-flex justify-content-center align-items-center">
                        <button class="mt-4 btn fw-bold text-white">LOGIN</button>
                    </div>
                    </form>         
                   </div>
        </section>

